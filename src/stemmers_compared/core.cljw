(defn get-dict-map [in-file]
  (with-open [rdr (io/reader in-file)]
    (zipmap (line-seq rdr) (repeat true))))
;(def dict-map (get-dict-map "./resources/nounlist.txt"))
(def dict-map (get-dict-map "./resources/wordsEn.txt"))
