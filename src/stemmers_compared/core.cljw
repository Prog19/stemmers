(def plurals (filter #(re-matches #".*s$" %) keywords))
;(count plurals)
(def plurals (take 100 plurals))
;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;stemmers-compared.core/plurals</span>","value":"#'stemmers-compared.core/plurals"}
