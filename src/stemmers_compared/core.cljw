;; @@
(defn get-hits [phrase-lst]
  (let [parsed-res (map 
              (fn[phrase]
    	  		(if (= (re-find #"\s" phrase) nil);single-term phrase
             	  phrase
             	  (subs (re-find #"\s[^\s]*$" phrase) 1))) phrase-lst);multi-term phrase
        hits (filter #(get dict-map %) parsed-res)
        misses (filter #(not (get dict-map %)) parsed-res)
        ret (filter #(not (.equals % "")) hits)
        ]
    (println "\nmiss: " misses)
    [(count ret) ret]))

;hits
(get-hits ["apple" "appl"])
(get-hits ["library" "librari" "libraries"]);plurals excluded in nounlist.txt
(get-hits ["ipod software update" "ipod software updates"])
(prn "this line is added as otherwise consecutive calls generating the same output returns that output only once")
(get-hits ["update" "updates"])
