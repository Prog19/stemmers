;; **
;;; Algorithmic stemmers are typically 4-5 x faster than Hunspell stemmers as they have to load all words and affixes into memory.
;;; A dictionary stemmer is only as good as its dictionary while algorithmic stemmers can process new words.
;;; Hunspell gives you greater flexibility than the algorithmic stemmers in that you can customize its dictionary.
;; **

;; @@
(defn hunspell-stem [variant]
 (let [affixStream (FileInputStream. (io/file "./resources/en_US.aff"));prefix rules removed as disk would be stemmed to k 
       dictStream (FileInputStream. (io/file "./resources/en_US.dic"))
       dict 
       (HunspellDictionary. affixStream dictStream (Version/LUCENE_36) true)
       stemmer (HunspellStemmer. dict)
       stems (.stem stemmer variant)]
   (if (> (.size stems) 0)
     (.getStemString (.get stems (dec (.size stems)))))))
(hunspell-stem "automaticaly opens itunes")

(get-stem hunspell-stem ["apples" "libraries"])
(get-stem hunspell-stem ["nanos videos" "automaticaly opens itunes"]); originally returned (nil nil) - doesn't process multi-term phrases
(get-stem hunspell-stem ["disks" "remarkable" "restore" "feet" "mice" "power" "organs" "organization"]);
