;; @@
;K stem
(def kstem-keywords-res (get-stem kstem plurals))
(def hits (get-hits kstem-keywords-res))
hits
;matched likely plurals
(filter #(re-matches #".*s$" %) (second hits))

;EnglishMinimal
(def en-min-stem-res (map #(luke-stem (EnglishMinimalStemmer.) % (count %)) plurals))
(def hits (get-hits en-min-stem-res))
hits
(filter #(re-matches #".*s$" %) (second hits))

;Hunspell
(def hunspell-stem-res (filter #(not (= % nil)) (get-stem hunspell-stem plurals)))
(def hits (get-hits hunspell-stem-res))
hits
(filter #(re-matches #".*s$" %) (second hits))
