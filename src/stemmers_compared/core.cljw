;; **
;;; Krovetz's(K) stemmer - light english stemming
;; **

;; @@
(def stream TokenStream)
(defn kstem [variant]
 (let [rdr (StringReader. variant)
       tokenizer (StandardTokenizer. (Version/LUCENE_36) rdr)
       fact (KStemFilterFactory.)
       stream (.create fact tokenizer)
       term (.addAttribute stream CharTermAttribute)]
  (.reset stream)
  (.incrementToken stream)
  (.toString term)))
(kstem "nanos videos")
(kstem "automatically opens itunes")
  
(defn get-stem [stem-fn variant-lst]
 (doall 
   (map
     (fn[variant]
  	  (if (> (count (clojure.string/split variant #" ")) 1);multi-term phrase
      (let [phrase-terms (clojure.string/split variant #" ")
            last-term (last phrase-terms)
            tmp (drop-last phrase-terms)
            stem (stem-fn last-term)]
        (if (not stem)
          variant
          (clojure.string/join #" " (concat tmp [stem]))))
      (let [stem (stem-fn variant)];single-term phrase
        (if (not stem)
          variant
          stem)))) variant-lst)))
     
(get-stem kstem ["apples" "libraries"])
(get-stem kstem ["nanos videos" "automatically opens itunes"]);originally returned ("nano" "automatic") - stems just the first terms of a phrase
