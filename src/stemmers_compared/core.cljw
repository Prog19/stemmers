;; **
;;; Lucene's minimal_english stemmer - for removing plurals
;; **

;; @@
(defn luke-stem
 ([inst variant]
   (let [truncate-index (.stem inst variant)]
     (subs variant 0 truncate-index)))
 ([inst variant len]
   (let [truncate-index (.stem inst (char-array variant) len)]
     (subs variant 0 truncate-index))))

;minimal english stemming
(defn en-min-stem [variant-lst]
  (map #(luke-stem (EnglishMinimalStemmer.) % (count %)) variant-lst))
(en-min-stem ["apples" "libraries"])
(en-min-stem ["nanos videos" "automatically opens iTunes"])
